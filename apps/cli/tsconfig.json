{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "types": [
            "node"
        ]
    },
    "include": [
        "src/**/*"
    ]
}